 : hideMeta;\n  const transition = {\n    duration: 0.25,\n    type: \"spring\",\n    bounce: 0.2,\n    ease: 'easeIn',\n  };\n\n\n  return (\n    <Card\n      onMouseEnter={() => setHovered(true)}\n      onMouseLeave={() => setHovered(false)}\n      key={data?.id}\n      onClick={open}\n    >\n      <CardMedia animate={animateCandMedia} transition={transition}>\n        <img src={data?.projectImage} alt=\"\" />\n        {data?.fundStatus === 2 && (\n          <Featured>\n            <FeatureTitle>ended!</FeatureTitle>\n          </Featured>\n\n        )}\n\n        <StatusRibbon fundStatus={data?.fundStatus}>\n          <Status>\n            {/* {data?.fundStatus === 1 ? \"Funding is Alive\" : \"Funding has ended\"} */}\n            {data?.fundStatus === 1 ? <CountDownTimer hoursMinSecs={data.timeLeft} /> : \"Funding has ended\"}\n          </Status>\n\n        </StatusRibbon>\n\n\n      </CardMedia>\n\n      <CardBody>\n        <CardTitle>\n          <h5>{data?.projectTitle}</h5>\n        </CardTitle>\n        {!isHovered && (\n          <MetaInfo animate={animeMeta}>\n            <Author>\n              <Avatar>\n                <img src={data?.projectImage}></img>\n              </Avatar>\n\n              <Info><span>Creator</span>\n                <h4>{data?.creatorName}</h4></Info>\n            </Author>\n            <PriceInfo>\n              <span>Funds Required</span>\n\n\n              <Price>\n                <PriceText>{data?.fundsRequired}</PriceText>\n              </Price>\n            </PriceInfo>\n\n\n          </MetaInfo>)}\n        <TagsCta anime={animeMetaList}>\n          <Tags>\n            <span>milestone</span>\n          </Tags>\n\n          <CtaGrp>\n            <CtaLink href=\"/\">Contract</CtaLink>\n            <CtaLink href=\"/\">Escrow</CtaLink>\n          </CtaGrp>\n        </TagsCta>\n        <MetaList animate={animeMetaList}>\n          <MetaListItem>\n            <div>Funds Required</div>\n            <div>{data?.metaList?.fund_1}</div>\n          </MetaListItem>\n          <MetaListItem>\n            <div>Funds Raised</div>\n            <div>{data?.metaList?.fund_2}</div>\n          </MetaListItem>\n          {/* <MetaListItem>\n        <div>Soft Cap</div>\n        <div>{data?.metaList?.sort_cap}</div>\n        </MetaListItem> */}\n          <MetaListItem>\n            <div>Investors</div>\n            <div>{data?.metaList?.investor_count}</div>\n          </MetaListItem>\n          <MetaListItem>\n            <div>Potential Returns</div>\n            <div>{data?.metaList?.ROI}%</div>\n          </MetaListItem>\n          <MetaListItem>\n          </MetaListItem>\n          <Link style={{ textDecoration: 'none' }} to={\"/project\" + data?.id}>\n            {/* {data?.fundStatus === 1 ? \"Funding is Alive\" : \"Funding has ended\"} */}\n            <Button>\n              Find Out More\n            </Button>\n          </Link>\n        </MetaList>\n        {/* <a href=\"https://github.com/tzw0/shoppeth/blob/master/src/components/topbar/Topbar.jsx\">wtf</a> */}\n        {/* <Link to=\"/detail\">\n          <button>\n              Find Out More\n          </button>\n            </Link> */}\n\n      </CardBody>\n\n\n    </Card>\n\n  )\n}\n\nconst Button = styled.button`\ndisplay: flex;\njustify-content: center;\nwidth: 100%;\npadding: 15px;\ncolor: white;\nbackground-color: #4717F6;\nbackground-image: linear-gradient(180deg, #4717F6 0%, #A2396A 80%);\nborder: none;\ncursor: pointer;\nfont-weight: bold;\nborder-radius: 8px;\nfont-size: large;\nmargin-top: -20px;\n`;\n\nexport default ProjectCard;","/Users/hamsterpapa/Documents/GitHub/Crowdfunding-Website/src/components/ProjectData.js",[],[],"/Users/hamsterpapa/Documents/GitHub/Crowdfunding-Website/src/components/sections/Projects.js",["146","147"],[],"import React from 'react';\nimport {useState} from 'react';\nimport styled from \"styled-components\";\n\nimport ProjectCard from '../ProjectCard';\nimport { ProjectContainer } from '../ProjectCardStyles';\nimport { ProjectData } from '../ProjectData';\n\n\nconst Section = styled.div`\n   min-height: 100vh;\n   width: 100vw;\n   background-color: #202020;\n   position: relative;\n   margin-top: -20px;\n`\n\nconst Title = styled.h1`\nfont-size: 40px;\ntext-transform: capitalize;\ncolor: ##fff;\ndisplay: flex;\njustify-content: center;\nalign-items: center;\nmargin: 1rem auto;\npadding-top: 120px;\nmargin-bottom: 50px;\nborder-bottom: 2px solid;\nwidth: fit-content;\n\n\n`\n\nconst Projects = () => {\n\n   const [open, setOpen] = useState(false);\n\n   const openModal = () => {\n    setOpen(true);\n  }\n\n   const closeModal = () => {\n    setOpen(false);\n  }\n\n  return (\n    <>\n    <Section id=\"projects\">\n      <Title>Latest Projects</Title>\n   \n      <ProjectContainer>\n        {ProjectData?.map((item) => {\n          return (\n            <>\n            <ProjectCard data={item} open={openModal}/>\n             {/* <Link to=\"home\" className='navbar-logo' spy={true} smooth={true} offset={-20} duration={500} onClick={closeMobileMenu}>\n          TRUEPNL  <i className=\"fa-solid fa-globe\"></i>\n        </Link> */}\n            {/* {open && \n            (<Overlay close={closeModal}>\n            <Modal data={item}  close={closeModal}/>\n            </Overlay>)\n            } */}\n            </>\n             )\n          })} \n      </ProjectContainer>\n          </Section>\n          </>\n  )\n}\n\nexport default Projects","/Users/hamsterpapa/Documents/GitHub/Crowdfunding-Website/src/components/sections/Getstarted.js",["148","149"],[],"import React from 'react';\nimport styled from \"styled-components\";\nimport { Button } from './Button';\nimport Typewriter from \"typewriter-effect\";\n\nconst Section = styled.div`\n   min-height: 100vh;\n   width: 100%;\n   background-color: #0C0032;\n   color: #fff;\n\n   display: flex;\n   justify-content: center;\n   align-items: center;\n   position: relative;\n  `\n\nconst Container = styled.div`\nwidth: 95%;